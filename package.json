{
  "name": "@pranavraut033/ats-checker",
  "version": "1.0.4",
  "description": "Deterministic, configurable ATS (Applicant Tracking System) compatibility checker with no external dependencies. Analyze resumes, generate scores, and get actionable suggestions.",
  "license": "MIT",
  "author": {
    "name": "Pranav Raut",
    "url": "https://github.com/Pranavraut033"
  },
  "contributors": [
    {
      "name": "Pranav Raut",
      "url": "https://github.com/Pranavraut033"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Pranavraut033"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pranavraut033/ats-checker"
  },
  "bugs": {
    "url": "https://github.com/Pranavraut033/ats-checker/issues"
  },
  "homepage": "https://github.com/Pranavraut033/ats-checker#readme",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "ats",
    "resume",
    "job-description",
    "scoring",
    "compatibility",
    "hiring",
    "checker",
    "analysis",
    "applicant-tracking"
  ],
  "scripts": {
    "build": "tsup && npm run build:ui",
    "build:ui": "mkdir -p ui/public/dist && cp dist/index.mjs ui/public/dist/ && cp dist/index.d.ts ui/public/dist/",
    "serve": "npx http-server ui/public -p 3005",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "express": "^4.18.2",
    "tsx": "^4.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  }
}
